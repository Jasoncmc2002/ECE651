{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-markdown": "^6.2.4",
    "@codemirror/lang-python": "^6.1.4",
    "@codemirror/language-data": "^6.4.1",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.26.0",
    "@monaco-editor/react": "^4.6.0",
    "@react-aria/ssr": "^3.9.7",
    "@reduxjs/toolkit": "^2.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@uiw/react-codemirror": "^4.21.24",
    "bootstrap": "^5.3.3",
    "browser-image-compression": "^2.0.2",
    "file-saver": "^2.0.5",
    "github-markdown-css": "^5.5.1",
    "jquery": "^3.7.1",
    "jwt-decode": "^4.0.0",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.2",
    "react-bootstrap-icons": "^1.11.4",
    "react-countdown": "^2.3.5",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-simple-code-editor": "^0.13.1",
    "redux": "^5.0.1",
    "redux-mock-store": "^1.5.5",
    "redux-thunk": "^3.1.0",
    "rehype-highlight": "^7.0.0",
    "rehype-katex": "^7.0.0",
    "remark-gfm": "^4.0.0",
    "remark-math": "^6.0.0",
    "web-vitals": "^2.1.4",
    "xlsx": "^0.18.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "coverage_1": "npm test -- --coverage --watchAll --collectCoverageFrom='src/components/*.jsx'",
    "coverage_2": "npm test -- --coverage --watchAll --collectCoverageFrom='src/components/contents/*.jsx'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{jsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/components/App.jsx"
    ],
    "coverageReporters": [
      "text"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!react-markdown|remark-gfm|remark-math|rehype-katex|rehype-highlight|prismjs|@marijn/find-cluster-break)/",
      "[/\\\\]node_modules[/\\\\].+[^esm]\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 1,
        "functions": 1,
        "lines": 1,
        "statements": 1
      }
    },
    "coveragePathIgnorePatterns": [
      "./node_modules/",
      "./src/App.jsx"
    ]
  },
  "devDependencies": {
    "ajv": "^7.2.4",
    "vitest": "^3.0.4"
  }
}
